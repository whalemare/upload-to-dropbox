    "dropbox": "^8.1.0",
    "globby": "^11.0.1",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/globby": "^9.1.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "@types/node-fetch": "^2.5.7",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "@vercel/ncc": "^0.25.1",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "jest-circus": "^26.6.3",
    "js-yaml": "^3.14.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
